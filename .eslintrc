{
  "parser": "babel-eslint",
  "plugins": ["react"],
  "env": {
        "node": true,
        "browser": true
  },
  "extends": "airbnb",
  "rules": {
        semi: [2, "never"] ,
        "import/no-unresolved": 0,
        "no-underscore-dangle": 0,
        "react/prop-types": [2,{ignore:["children","className",'style']}],
        "no-param-reassign": 0,
        "global-require": 0,
        "no-console": 0,
  }
}
